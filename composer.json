{
    "name": "hopasports/blade-sporttype-icons",
    "type": "library",
    "description": "A package to easily make use of Hopasports Sporttype icons in your Laravel Blade views.",
    "homepage": "https://github.com/hopasports/blade-sporttype-icons",
    "license": "CC0-1.0",
    "authors": [
        {
            "name": "Eddie Palmans",
            "email": "eddie@hopatec.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Hopasports\\BladeSporttypeIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hopasports\\BladeSporttypeIcons\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hopasports\\BladeSporttypeIcons\\BladeSporttypeIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
